COLUMN TABLE "data.db.tables::Promociones"(
	"ID_PROMOCION" CHAR(10) CS_FIXEDSTRING NOT NULL,
	"ID_PRESENTACION" CHAR(10) CS_FIXEDSTRING,
	"ID_INGREDIENTE" CHAR(10) CS_FIXEDSTRING,
	"ID_PROVEEDOR" CHAR(10) CS_FIXEDSTRING,
	"PRECIO_PROMO" Decimal(10,2),
	"FECHA_INICIO" DATE,
	"VIGENCIA" DATE,
	PRIMARY KEY (
		"ID_PROMOCION",
		"ID_PRESENTACION",
		"ID_INGREDIENTE",
		"ID_PROVEEDOR"
	)
)
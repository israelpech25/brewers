COLUMN TABLE "data.db.tables::Sales"(
	"ID_VENTA" CHAR(10) CS_FIXEDSTRING NOT NULL,
	"ID_CERVEZA" CHAR(10) CS_FIXEDSTRING NOT NULL,
	"NOMBRE_CLIENTE" CHAR(40) CS_FIXEDSTRING NOT NULL,
	"FECHA_VENTA" DATE NOT NULL,
	"UNIDADES_VENDIDAS" INTEGER CS_INT NOT NULL,
	PRIMARY KEY (
		"ID_VENTA",
		"ID_CERVEZA",
		"NOMBRE_CLIENTE"
	)
)